{
  "open_account": [
    [
      {
          "slot": "end_talk",
          "condition": "true",
          "not_ask": 1
      },
      {"action": "goto: bye"}
    ],
    [
      {
        "slot": "currency",
        "condition": "supported_currency"
      },
      [
        {
          "slot": "currency",
          "condition": "not_eq",
          "value": "rub"
        },
        {
          "slot": "online_reserving",
          "condition": "true",
          "not_ask": 1
        },
        {"action": "say: cant_reserve; goto: search_bank"}
      ],
      [
        {"true": "show_docs"},
        "resident",
        "client_type",
        {"action": "say: new_acc_documents_list"}
      ],

      [
        {"true": "cost_of_service"},
        "region",
        {"action": "say: new_acc_rates_list"}
      ]
    ],

    [
      {
        "slot": "currency",
        "condition": "not_supported_currency"
      },
      {"action": "say: not_supported; goto: "}
    ],

    [
      {
        "slot": "currency",
        "condition": "not_eq",
        "value": "rub"
      },
      {"action": "say: send_to_bank_curr; goto: search_bank"}
    ],

    [
      {
        "slot": "account_type",
        "condition": "not_in",
        "value": "расчетный счет,гособрзаказ",
        "not_ask": 1
      },
      {
        "slot": "online_reserving",
        "condition": "true",
        "not_ask": 1
      },
      {"action": "say: cant_reserve_acc_type; goto: search_bank"}
    ],

    [
      {
        "slot": "currency",
        "condition": "eq",
        "value": "rub"
      },
      {
        "slot": "account_type",
        "condition": "in",
        "value": "расчетный счет,гособрзаказ"
      },
      {"true": "online_reserving"},
      {"action": "say: reserve_new_acc_online; goto:"}
    ],

    {"action": "say: send_to_bank; goto: search_bank"}
  ],

  "search_bank": [
    [
      {
          "slot": "end_talk",
          "condition": "true",
          "not_ask": 1
      },
      {"action": "goto: bye"}
    ],
    [
      {"true": "search_vsp"},
      "method_location",
      {
        "action": "say: show_vsp",
        "relevant_slots": ["method_location", "client_address", "client_geo", "client_metro", "show_schedule", "show_phone"]
      }
    ],
    {"true": "end_talk"}
  ],

  "no_intent": [
    [
      {
          "slot": "end_talk",
          "condition": "true",
          "not_ask": 1
      },
      {"action": "goto: bye"}
    ],
    {"action": "say: no_intent; goto:"}
  ],

  "bye": [
    {"action": "say: bye; goto:"}
  ]
